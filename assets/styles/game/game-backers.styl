.game__backers
	width 100%
	display flex
	flex-direction column
	margin-top 50px

	&-title
		font-size 24px
		font-weight $bold

	&-list
		display flex
		flex-wrap wrap
		row-gap $gap-small
		column-gap $gap-small
		margin-top $gap

	&-item
		color $color-text
		font-size 20px

		& .ui-avatar
			margin-right $gap-small
